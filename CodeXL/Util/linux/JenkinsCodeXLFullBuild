#!/bin/sh
# Top level jenkins script to build for promoted AMDT libraries, and verify
# that the built results have all of the expected bits.
# Also generate the CodeXL-Linux[32|64].xml file for the updater to consume
#
# Usage:
#   $0 [<version number>] [scratch]
#   <version number>:
#       An optional parameter is the build number.
#       When the build number is 0 (zero) only the debug version is built
#       If no build number is passed in, do not modify the version file
#   scratch:
#       An optional parameter.  When passed in, the script will run
#       scons <stuff> -c to perform a clean prior to the normal build
#       Thus, a scratch build
#

#
# Assumptions:
# - WORKSPACE environment variable to be set
#

AMD_PERFORCE_ROOT=$WORKSPACE
AMD_BRANCH_PART=main
AMD_BRANCH_ROOT=${AMD_PERFORCE_ROOT}/${AMD_BRANCH_PART}
AMD_WS_LINUX_UTIL_DIR=${AMD_BRANCH_ROOT}/CodeXL/Util/linux
CODEXL_HELP_FOLDER=${AMD_BRANCH_ROOT}/CodeXL/Help

# NOTE: This is used by buildCodeXLFullLinuxProjects
export AMD_CODEXL=$AMD_BRANCH_ROOT/CodeXL
export AMD_OUTPUT=$AMD_BRANCH_ROOT/

# Version Number stuff
VERSION_MAJOR=2
VERSION_MINOR=0
VERSION_BUILD=0
# VERSION_UPDATE=0
VERSIONINFO_FILE=${AMD_CODEXL}/../CommonProjects/VersionInfo/VersionInfo.h

NUM_ERRORS=0
BUILD_RELEASE_RC=0
GPUP_BUILD_RC=0
IS_SCRATCH_BUILD=0

export CONFIG_FILE_BASE=CodeXLUpdate_linux

# A utility bit of code - simply does a scons -c over both builds
NukeIt()
{
    ARCHTYPE=$1
    BUILD_OPTS="--debug=time -j ${NCPUS} CXL_build=release CXL_arch=${ARCHTYPE} -c"
    sh $AMD_WS_LINUX_UTIL_DIR/buildCodeXLFullLinuxProjects ${BUILD_OPTS} > ${WORKSPACE}/CodeXL-Linux_${ARCHTYPE}_release.log 2>&1
}

# The bulk of the work is done here
# The only parameter is the architecture name (x86 or x86_64)
RunTheBuild()
{
    ARCHTYPE=$1
    BUILDTYPE=$2

    cd $AMD_WS_LINUX_UTIL_DIR

    # BUILD RELEASE when the version is non-zero
    BUILD_OPTS="--debug=time -j ${NCPUS} CXL_build=release CXL_arch=${ARCHTYPE} CXL_build_conf=${BUILDTYPE}"
    echo ""
    echo "========== Timing start: `date` ============================================"
    echo "Building: sh $AMD_WS_LINUX_UTIL_DIR/buildCodeXLFullLinuxProjects ${BUILD_OPTS}"
    sh $AMD_WS_LINUX_UTIL_DIR/buildCodeXLFullLinuxProjects ${BUILD_OPTS} > ${WORKSPACE}/CodeXL-Linux_${ARCHTYPE}_release.log 2>&1
    BUILD_RELEASE_RC=$?
    echo "========== Timing end: `date` =============================================="

    # BUILD RELEASE x86 when the version is non-zero
    BUILD_OPTS="x86 --debug=time -j ${NCPUS} CXL_build=release CXL_arch=${ARCHTYPE} CXL_build_conf=${BUILDTYPE}"
    echo ""
    echo "========== Timing start: `date` ============================================"
    echo "Building: sh $AMD_WS_LINUX_UTIL_DIR/buildCodeXLFullLinuxProjects ${BUILD_OPTS}"
    sh $AMD_WS_LINUX_UTIL_DIR/buildCodeXLFullLinuxProjects ${BUILD_OPTS} > ${WORKSPACE}/CodeXL-Linux_x86_release.log 2>&1
    BUILD_RELEASE_RC=$?
    echo "========== Timing end: `date` =============================================="

    GPUPROFILER_BACKEND_BUILD=${AMD_BRANCH_ROOT}/CodeXL/Components/GpuProfiling/Build
    GPUPROFILER_BACKEND_BUILD_SCRIPT=${GPUPROFILER_BACKEND_BUILD}/backend_build.sh

    GPUP_BUILD_OPTS="bldnum ${VERSION_BUILD} workspace ${AMD_PERFORCE_ROOT} release-root ${AMD_BRANCH_PART}"
    if [ ${BUILDTYPE} = "INTERNAL" ]
    then
       GPUP_BUILD_OPTS=$GPUP_BUILD_OPTS" build-internal-only"
    fi
    GPUP_BUILD_OPTS=$GPUP_BUILD_OPTS" skip-unit-tests"

    GPUP_BUILD_INCREMENTAL=
    if [ ${IS_SCRATCH_BUILD} -eq 0 ]
    then
        GPUP_BUILD_INCREMENTAL="incremental"
    fi
    echo ""
    echo "========== Timing start: `date` ============================================"
    echo "Building: sh ${GPUPROFILER_BACKEND_BUILD_SCRIPT} ${GPUP_BUILD_OPTS} ${GPUP_BUILD_INCREMENTAL}"
    sh ${GPUPROFILER_BACKEND_BUILD_SCRIPT} ${GPUP_BUILD_OPTS} ${GPUP_BUILD_INCREMENTAL}
    GPUP_BUILD_RC=$?
    echo "========== Timing end: `date` =============================================="

    NUM_ERRORS=`expr ${NUM_ERRORS} + ${BUILD_RELEASE_RC} + ${GPUP_BUILD_RC}`
    if [ ${BUILD_RELEASE_RC} -ne 0 -o ${GPUP_BUILD_RC} -ne 0 ]
    then
        echo "*** ERROR ***"
        echo "*** The GPU Profiler backend build had ${GPUP_BUILD_RC} errors ***"
        if [ ${GPUP_BUILD_RC} -ne 0 ]
        then
            echo "***    Check ${GPUPROFILER_BACKEND_BUILD}/GpuProfilerBackend_Build.log for errors ***"
        fi


        echo "*** The release build had ${BUILD_RELEASE_RC} errors ***"
    else
	echo "Build succeeded"
    fi

}

PackageTheBuild()

{
    set -e
    BUILDTYPE=$1

    echo "---- TIMING: Packaging start: `date`"

    cd $AMD_BRANCH_ROOT

    # First: clean out object files and other build artifacts which should not go into
    # the final tarball
    rm -rf Output_x86_64/release/obj

    # Future enhancement:
    # This is a good point to split in the future.  Generate the CodeXL tarball bits
    # and do not bother with other components until some other repackaging step.
    # This will keep the overhead and complexity lower here.

    # Extract the user guide html files from their archive into the expected output folder
    cd $AMD_BRANCH_ROOT/Output_x86_64/release/bin
    mkdir -p webhelp
    cd $CODEXL_HELP_FOLDER
    tar -xvf "./CodeXL_User_Guide_HTML.tar.gz" -C $AMD_BRANCH_ROOT/Output_x86_64/release/bin/webhelp

    cd $AMD_BRANCH_ROOT

	if [ ${BUILDTYPE} = "PUBLIC" ]

	then
		################################################################################################
		# Copy the files for the CodeXL Public version
		################################################################################################
		PKG_NAME=AMD_CodeXL_Linux_x86_64_${VERSION_MAJOR}.${VERSION_MINOR}.${VERSION_BUILD}
		mkdir -p ${PKG_NAME}
		cp ${AMD_CODEXL}/../CodeXL/Installer-Linux/debain/main/amdcodexlicon.desktop ${PKG_NAME}
		cp ${AMD_CODEXL}/../CodeXL/Installer-Linux/debain/main/amdremoteagenticon.desktop ${PKG_NAME}
		cp -f Output_x86_64/release/bin/AMD_CodeXL_Release_Notes.pdf             ${PKG_NAME}
		cp -f Output_x86_64/release/bin/AMDTRemoteDebuggingServer                ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXL                                   ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLAnalyzer                           ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLAnalyzer-bin                       ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXL-bin                               ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLCpuProfiler                        ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLCpuProfiler-bin                    ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLRemoteAgent-bin                    ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLPowerProfiler                      ${PKG_NAME}
		cp -f Output_x86_64/release/bin/CodeXLPowerProfiler-bin                  ${PKG_NAME}
		cp -f Output_x86_64/release/bin/AMDTSystemInformationHelper-x64          ${PKG_NAME}
		cp -f Output_x86/release/bin/AMDTSystemInformationHelper                 ${PKG_NAME}
		cp -f Output_x86_64/release/bin/AMDTPwrProfDriver.sh                     ${PKG_NAME}
		cp -f Output_x86_64/release/bin/AMDTPwrProfDriverSource.tar.gz           ${PKG_NAME}
		cp -f Output_x86_64/release/bin/AMDTPwrProfVersion                       ${PKG_NAME}
		cp ${AMD_CODEXL}/../CommonProjects/AMDTRemoteAgent/CodeXLRemoteAgentConfig.xml ${PKG_NAME}
		cp ${AMD_CODEXL}/../CommonProjects/AMDTRemoteAgent/CodeXLRemoteAgent     ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDOpenCLDebugAPI64.so               ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTAnalysisBackend.so               ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTAnalyzer.so                      ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTDbAdapter.so                     ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTProfilerDAL.so                   ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTPowerProfilingMidTier.so         ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTPowerProfiling.so                ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTAPIClasses.so                    ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTApiFunctions.so                  ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTApplicationComponents.so         ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTApplicationFramework.so          ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTApplication.so                   ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTAssertionHandlers.so             ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTBaseTools.so                     ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTCpuCallstackSampling.so          ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTCpuPerfEventUtils.so             ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTCpuProfilingControl.so           ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTCpuProfilingRawData.so           ${PKG_NAME}
		cp -df Output_x86_64/release/bin/libAMDTCpuProfiling.so                  ${PKG_NAME}
